
/plugins/ServerJam/config.yaml
{
	uuid: "{player-uuid}"
	name: "{name}"
	level: {level}
	exp: {exp}
	coins: {coins}
	friends: ["{friend1-uuid}", "{friend2-uuid}", "{friend3-uuid}"]
	maps: {
		{map1}: ["default"],
		{map2}: ["default", "adventure"],
		{map3}: ["default", "adventure", "apocalypse"],
		{map4}: ["default", "apocalypse"],
	}
	runes: ["rune-map", "rune-map2"]
}

/plugins/ServerJam/camps/{player-uuid}-camp.yaml
{
	world: {world-name}
	x: {x}
	y: {y}
	z: {z}
}


/plugins/ServerJam/items/{item}.yaml
{
	material: "{material}"
	displayname: "{displayname}"
	lore:
		0: "----"
		1: "----"
		2: "----"
		3: "----"
}